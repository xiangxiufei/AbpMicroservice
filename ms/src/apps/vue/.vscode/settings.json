{
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": ["source.fixAll.eslint"],
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "vue"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "i18n-ally.localesPaths": "src/locales/define",
  "i18n-ally.enabledParsers": ["json", "ts"], //这个最好加上，如果是其他格式，如ts，不加上就无效了
  "i18n-ally.sourceLanguage": "zh-CN", // 翻译源语言
  "i18n-ally.displayLanguage": "zh-CN",
  "scss.lint.unknownAtRules": "ignore",

  "volar.inlayHints.eventArgumentInInlineHandlers": false,
  "gitmoji.onlyUseCustomEmoji": true,
  "gitmoji.outputType": "code",
  "gitmoji.addCustomEmoji": [
    { "emoji": "⭐", "code": "feat: ", "description": "feat: 新功能、新特性" },
    { "emoji": "🐛", "code": "fix: ", "description": "fix: 修改 bug" },
    { "emoji": "⚡️", "code": "perf: ", "description": "perf: 更改代码，以提高性能" },
    { "emoji": "♻️", "code": "refactor: ", "description": "refactor: 代码重构" },
    { "emoji": "📄", "code": "docs: ", "description": "docs: 文档修改" },
    { "emoji": "🎨", "code": "style: ", "description": "style: 代码格式修改, 注意不是 css 修改" },
    { "emoji": "✅", "code": "test: ", "description": "test: 测试用例新增、修改" },
    { "emoji": "🙅‍", "code": "build: ", "description": "build: 影响项目构建或依赖项修改" },
    { "emoji": "🔙", "code": "revert: ", "description": "revert: 恢复上一次提交" },
    { "emoji": "💚", "code": "ci: ", "description": "ci: 持续集成相关文件修改" },
    { "emoji": "🤡", "code": "chore: ", "description": "chore: 其他修改（不在上述类型中的修改）" },
    { "emoji": "📦", "code": "release: ", "description": "release: 发布新版本" }
  ],
  "gitmoji.showEmojiCode": false,
  "vue3snippets.enable-compile-vue-file-on-did-save-code": false,
  "i18n-ally.keystyle": "nested",
  "cSpell.words": [
    "abpui",
    "Administration",
    "alive",
    "boot",
    "Btns",
    "caching",
    "card",
    "Checkbox",
    "chip",
    "clearable",
    "commitlint",
    "Confirmed",
    "correlation",
    "Crud",
    "cruds",
    "curd",
    "Defines",
    "descending",
    "Dismiss",
    "easy",
    "Emits",
    "gears",
    "identitys",
    "intlify",
    "Invokes",
    "layouts",
    "localization",
    "Localizer",
    "lockout",
    "managements",
    "oidc",
    "outlined",
    "Pager",
    "Pages",
    "Pagination",
    "persistedstate",
    "pinia",
    "postversion",
    "qtable",
    "quasar",
    "Recordable",
    "sass",
    "Saving",
    "signalr",
    "slots",
    "sortable",
    "submited",
    "surname",
    "Suspense",
    "swipeable",
    "tenant",
    "tenants",
    "Unauthorized",
    "unplugin",
    "Vaildation",
    "vueuse",
    "windicss",
    "zyknow"
  ],
  "vue.complete.casing.props": "kebab",
  "vue.complete.casing.tags": "kebab",
  "vue.server.fullCompletionList": true,
  "vue.server.vitePress.supportMdFile": false,
  "vue.updateImportsOnFileMove.enabled": true,
  "vue.inlayHints.inlineHandlerLeading": false,
  "compile-hero.disable-compile-files-on-did-save-code": true,
  "cSpell.language": "zh,en,en-US,en-GB,lorem"
}
