{
  "Apollo": {
    "AppId": "InternalGateway",
    "ConfigServer": [
      "http://localhost:18080"
    ],
    "Namespaces": [
      "appsettings.json",
      "yarp.json"
    ]
  },
  "App": {
    "SelfUrl": "https://localhost:43000",
    "CorsOrigins": "https://*.AbpMicroservice.com,http://localhost:42000,https://localhost:40100,https://localhost:40000"
  },
  "AuthServer": {
    "Authority": "https://localhost:42000",
    "RequireHttpsMetadata": "true",
    "SwaggerClientId": "InternalGateway_Swagger"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ElasticSearch": {
    "Url": "http://localhost:9200"
  },
  "Jaeger": {
    "Enabled": "true",
    "Host": "localhost",
    "Port": "6831"
  }
}
